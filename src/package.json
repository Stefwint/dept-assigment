    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "next": "^9.2.0",
    "next-offline": "^4.0.5",
    "next-optimized-images": "^2.5.3",
    "nookies": "^2.0.8",
    "prettier": "^1.19.0",
    "react": "^16.8.1",
    "react-dom": "^16.12.0",
